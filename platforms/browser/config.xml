<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.notifMe" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>notifMe</name>
    <description>
        A notification app that tells you whenever there is a nearby place you are looking for based on google maps types of places.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
Guillermo Pradas	    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
